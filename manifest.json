{
    "manifest_version" : 2,
    "name" : "Experimental chrome extension",
    "description" : "Experimental chrome extension",
    "version" : "1.0",
    "content_scripts": [
        {
            "matches" : [
                "https://twitter.com/*"
            ],
            "js" : ["contentScript.js"]
        }
    ],
    "background" : {
        "scripts" : ["background.js"],
        "persistent" : false
    },
    "permissions": ["contextMenus", "tabs"]
}